<script type="text/javascript">
    $(function () {
        scope.LoadHelpCenterList();
    });

    var scope = (function (obj) { return obj; })(new function () {

        /*!
        * 加载帮助中心数据列表
        */
        function LoadHelpCenterList() {
            common.post("/Client/ClientCenter/HelpCenterList", null, function (r) {
                var html = [];
                $(r.Data).each(function () {
                    html.push('<div class="two_port_align ">');
                    html.push('<a href="/Client/ClientCenter/HelpCenterDeatil?ID='+this.ID+'">');
                    html.push('<div class="input_text co333">' + this.sTitle + '</div>');
                    html.push('<div><span class="fa fa-angle-right co333"></span></div>');
                    html.push('</a>');
                    html.push('</div>');
                });
                $('#HelpCenterList').append(html.join(''));
            });
        }

        return {
            LoadHelpCenterList: LoadHelpCenterList
        }

    });
</script>