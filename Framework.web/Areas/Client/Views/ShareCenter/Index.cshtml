<script src="~/scripts/lib/client.common.js"></script>
<script type="text/javascript">
    $(function () {
        scope.bingEvent();
        scope.GetShareInfo();
    });

    var scope = (function (obj) { return obj; }(new function () {

        //获取分享客的相关信息
        function GetShareInfo() {
            common.post("/Client/ShareCenter/GetShareInfo", null, function (r) {
                var json = r.Data;
                json.dPriceCount = json.dPriceCount || 0;
                $('.OrderCount').html("" + json.OrderCount + "<span>笔</span>");
                $('.dPriceCount').html("" + json.dPriceCount.toFixed(2) + "<span>元</span>");
                $('.MonthClientCount').text(json.MonthClientCount);
                $('.ClientCount').text(json.ClientCount);
                $('.predIncome').text(json.predIncome.toFixed(2));//预计收入
                common.setStorageItem("_dPriceCount", json.dPriceCount.toFixed(2));
            });
        }

        //绑定事件
        function bingEvent() {

            $('.willMoney').on("click", function () { location.href = "/Client/ShareCenter/AccountCenter" })//结算中心

            $('.Protocol').on("click", function () { location.href = "/Client/ShareCenter/Protocol?sTitle=关于分享客" })//分享客须知

            $('.Member').on("click", function () { location.href = "/Client/ShareCenter/Member" })//我的会员

            $('.ShopManagement').on("click", function () { location.href = "/Client/ShareCenter/ShopManagement" })//分享店铺管理

            $('.SharePoster').on("click", function () { location.href = "/Client/ShareCenter/SharePoster" })//我的分享海报


            $('.newlyMember').on("click", function () { location.href = "/Client/ShareCenter/NewlyMember" })//本月新增会员

            $('.PaymentDetail').on("click", function () { location.href = "/Client/ShareCenter/PaymentDetail" })//收入明细

            
        }



        return {
            bingEvent: bingEvent,
            GetShareInfo:GetShareInfo
        }
    }));

    //cdk复制码前端：

</script>